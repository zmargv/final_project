<div class="page-header">
  <h3>C Vote #<%= @c_vote.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Up</dt>
      <dd><%= @c_vote.up %></dd>

      <dt>User</dt>
      <dd><%= @c_vote.user_id %></dd>

      <dt>Comment</dt>
      <dd><%= @c_vote.comment_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/c_votes" class="btn btn-primary">
        Back
      </a>
      <a href="/c_votes/<%= @c_vote.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_c_vote/<%= @c_vote.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
