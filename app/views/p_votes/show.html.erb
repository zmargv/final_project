<div class="page-header">
  <h3>P Vote #<%= @p_vote.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Up</dt>
      <dd><%= @p_vote.up %></dd>

      <dt>User</dt>
      <dd><%= @p_vote.user_id %></dd>

      <dt>Post</dt>
      <dd><%= @p_vote.post_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/p_votes" class="btn btn-primary">
        Back
      </a>
      <a href="/p_votes/<%= @p_vote.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_p_vote/<%= @p_vote.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
